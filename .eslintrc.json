{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine": "auto"
        }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}

